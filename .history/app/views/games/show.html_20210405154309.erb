<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @game.title %>
</p>

<p>
  <strong>Developer:</strong>
  <%= @game.developer %>
</p>

<p>
  <th scope="col"><strong>Genre Name</strong></th>
  <th scope="col"><strong>Genre Description</strong></th>
</p>
<tbody>
  <% @game.genres.each do |genre| %>
      <tr>
        <td><%= genre.name %></td>
        <td><%= genre.description %></td>
      </tr>
      <% end %>
</tbody>

<br>

<%= link_to 'Edit', edit_game_path(@game) %> |
<%= link_to 'Back', games_path %>
